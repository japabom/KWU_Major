        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="40.0">
            <children>
                <Label text="이름" />
                <TextField fx:id="textfield1" prefHeight="23.0" prefWidth="110.0" />
            </children>
        </HBox>
        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="40.0">
            <children>
                <Label text="국어" />
                <TextField fx:id="textfield2" prefHeight="23.0" prefWidth="110.0" />
            </children>
        </HBox>
        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="40.0">
            <children>
                <Label text="수학" />
                <TextField fx:id="textfield3" prefHeight="23.0" prefWidth="110.0" />
            </children>
        </HBox>
        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="40.0">
            <children>
                <Label text="영어" />
                <TextField fx:id="textfield4" prefHeight="23.0" prefWidth="110.0" />
            </children>
        </HBox>
        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="40.0">
